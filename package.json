{
  "name": "swgoh",
  "version": "0.4.1",
  "description": "Unofficial node library for swgoh.gg",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/pikax/swgoh.git",
  "author": "pikax",
  "license": "MIT",
  "scripts": {
    "dist": "rollup -c",
    "test": "jest",
    "test:watch": "cross-env mock=1 jest --watch",
    "coverage": "jest --coverage",
    "snapshot": "jest --updateSnapshot",
    "static": "ts-node tools/build_static.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "coverageDirectory": "coverage",
    "mapCoverage": true
  },
  "dependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/request": "^2.47.0",
    "cheerio": "^1.0.0-rc.2",
    "promise-queue": "^2.2.5",
    "request": "^2.83.0",
    "requestretry": "^1.13.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "coveralls": "^3.0.0",
    "jest": "^22.1.4",
    "rollup": "^0.55.1",
    "rollup-plugin-typescript2": "^0.10.0",
    "ts-jest": "^22.0.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "optionalDependencies": {
    "cross-env": "^5.1.3"
  }
}
